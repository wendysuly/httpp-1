-include ../makefile.defs

# Add inputs and outputs from these tool invocations to the build variables 

# All Target
all: libhttpp.so

# Tool invocations
libhttpp.so: $(OBJS) $(USER_OBJS)
	@echo 'Building target: $@'
	@echo 'Invoking: GCC C++ Linker'
	g++ -shared -o"libhttpp.so" $(OBJS) $(USER_OBJS) $(LIBS)
	@echo 'Finished building target: $@'
	@echo ' '
	$(MAKE) --no-print-directory post-build

# Other Targets
clean:
	-$(RM) $(OBJS)$(C++_DEPS)$(C_DEPS)$(CC_DEPS)$(LIBRARIES)$(CPP_DEPS)$(CXX_DEPS)$(C_UPPER_DEPS) libhttpp.so
	-@echo ' '

post-build:
	-@echo 'Copy library to dir'
	-gksudo cp libhttpp.so /usr/lib
	-@echo ' '

install:
	@echo "Copying library to /usr/local/lib..."
	cp libhttp.so /usr/local/lib
	@echo "Copying sources to /usr/local/include..."
	cp ../*.hpp ../*.cpp /usr/local/include

.PHONY: all clean dependents
.SECONDARY: post-build

-include ../makefile.targets
